@media (prefers-color-scheme: dark) {
  .memo-link-text {
    color: rgb(0, 105, 12);
  }

  .methed {
    filter: brightness(0.5);
  }

  .todo {
    color: rgb(118, 51, 0);
  }

  .done {
    color: rgb(0, 105, 12);
  }

  .header {
    color: rgb(55, 55, 55);
    background: rgba(24, 24, 24, 0.5019607843137255);
  }

  .icon {
    border: rgb(0, 0, 0);
  }

  .none {
    color: rgba(255, 255, 255, 0.3137254901960784);
  }
}