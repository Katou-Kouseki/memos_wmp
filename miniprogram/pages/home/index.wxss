/* pages/home/index.wxss */
page {
  opacity: 0;
  animation: show 500ms 500ms forwards;
}

.btns {
  display: flex;
  margin-bottom: 30rpx;
}

.shareImg_block {
  width: 100%;
  height: 100%;
  background-color: #00000020;
  backdrop-filter: blur(30rpx);
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 6;
  overflow:hidden
}

.shareImg {
  width: 600rpx;
  height: 800rpx;
  background-color: #cccccc;
  border-radius: 30rpx;
  opacity: 0;
  transform: rotateY(90deg);
  animation: shareImg 300ms 300ms ease-in-out forwards;
}

.inputTypeBtn {
  margin-left: 50rpx;
  height: 70rpx;
  display: flex;
  align-items: center;
}

.typeBtn {
  width: 76rpx;
  height: 48rpx;
  background-size: cover;
}

.todoBtn {
  background-image: url(data:image/svg+xml;base64,);
}

.fullScrBtn {
  background-image: url(data:image/svg+xml;base64,);
}

.tagBtn {
  background-image: url(data:image/svg+xml;base64,);
}

.codeBtn {
  background-image: url(data:image/svg+xml;base64,);
}

.counter-block {
  margin: 10rpx;
}

.method {
  display: flex;
  position: absolute;
  right: 60rpx;
}

.method .btn {
  background-size: cover;
  margin-left: 20rpx;
  width: 50rpx;
  height: 50rpx;
}

.method .top {
  background-image: url(data:image/svg+xml;base64,);
}

.method .delete {
  background-image: url(data:image/svg+xml;base64,);
}

.method .edit {
  background-image: url(data:image/svg+xml;base64,);
}

.memo-link-text {
  color: #07C160;
}

.header {
  z-index: 5;
  position: fixed;
  display: flex;
  align-items: center;
  color: #cccccc;
  background: #f5f5f580;
  backdrop-filter: blur(30px);
  padding: 5rpx 20rpx;
  border-radius: 99rpx;
  opacity: 0;
  animation: show 300ms 100ms forwards;
}

.icon {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  border: 1rpx solid #cccccc50;
  margin-right: 20rpx;
}

.canvas {
  position: absolute;
  left: 750rpx;
  width: 0;
  height: 0;
}

@keyframes shareImg {
  from{
    opacity: 0;
    transform: rotateY(90deg);
  }

  to {
    opacity: 1;
    transform: rotateY(0deg);
  }
}


@import 'index-wxa-auto-dark.wxss'